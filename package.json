{
  "name": "chromium",
  "version": "2.2.0",
  "description": "Chromium binaries for Node.js projects",
  "repository": "dtolstyi/node-chromium",
  "main": "index.js",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "postinstall": "node install.js",
    "test": "xo && ava --verbose",
    "test-only": "ava --verbose"
  },
  "files": [
    "cache.js",
    "config.js",
    "index.js",
    "install.js",
    "utils.js"
  ],
  "keywords": [
    "chromium",
    "chrome",
    "browser"
  ],
  "author": "Dmytro TOLSTYI <dmytro.tolstyi@gmail.com>",
  "license": "MIT",
  "ava": {
    "timeout": "15m"
  },
  "xo": {
    "space": 4
  },
  "dependencies": {
    "debug": "^4.1.0",
    "extract-zip": "^1.7.0",
    "got": "^7.1.0",
    "rimraf": "^2.7.1",
    "tmp": "0.0.33",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "ava": "^3.11.0",
    "xo": "^0.32.1"
  }
